{
  "name": "ixnode",
  "productName": "IXnode",
  "version": "0.0.1",
  "description": "IXmaps client",
  "license": "TBD",
  "main": "main.js",
  "repository": "ixmaps/ixnode",
  "author": {
    "name": "ixmaps team",
    "url": "ixmaps.ca"
  },
  "engines": {
    "node": ">=2.0.1"
  },
  "scripts": {
    "build-electron": "node_modules/.bin/electron-rebuild -f",
    "start": "electron .",
    "debug": "electron --debug=5858 .",
    "package-linux": "electron-packager . ixnode --out dist/linux --ignore=node_modules/electron-prebuilt --ignore=node_modules/electron-packager --ignore=.git --platform=linux --arch=x64 --version=0.27.1 #--asar",
    "package-darwin": "electron-packager . ixnode --out dist/darwin --ignore=node_modules/electron-prebuilt --ignore=node_modules/electron-packager --ignore=.git --platform=darwin --arch=x64 --version=0.27.1 #--asar",
    "package-win32": "electron-packager . ixnode --out dist/win32 --ignore=node_modules/electron-prebuilt --ignore=node_modules/electron-packager --ignore=.git --platform=win32 --arch=x64 --version=0.27.1 #--asar",
    "test": "node test/*/*"
  },
  "files": [
    "main.js",
    "index.html",
    "index.css"
  ],
  "keywords": [
    "electron-app"
  ],
  "dependencies": {
    "async": "^1.0.0",
    "babel": "^5.4.7",
    "bootstrap": "^3.3.4",
    "cheerio": "^0.19.0",
    "lodash": "^3.7.0",
    "raw-socket2": "^1.2.5",
    "react": "^0.13.3",
    "react-bootstrap": "^0.23.2",
    "request": "^2.55.0"
  },
  "devDependencies": {
    "electron-packager": "^4.0.2",
    "electron-prebuilt": "^0.26.1",
    "electron-rebuild": "^0.2.2",
    "fs": "0.0.2",
    "gulp": "^3.8.11",
    "gulp-less": "^3.0.3",
    "gulp-notify": "^2.2.0",
    "gulp-shell": "^0.4.1",
    "gulp-streamify": "0.0.5",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "^3.0.4",
    "mkdirp": "^0.5.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.2.1"
  }
}
